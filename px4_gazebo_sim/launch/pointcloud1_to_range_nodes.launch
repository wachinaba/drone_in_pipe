<?xml version="1.0"?>
<launch>
  <node name="multizone_lidar_left_converter" pkg="multizone_lidar"
    type="pointcloud1_to_range_node" output="screen">
    <remap from="pointcloud" to="multi_zone_lidar_left/scan" />
    <remap from="range" to="multi_zone_lidar_left/range" />
  </node>
  <node name="multizone_lidar_right_converter" pkg="multizone_lidar"
    type="pointcloud1_to_range_node" output="screen">
    <remap from="pointcloud" to="multi_zone_lidar_right/scan" />
    <remap from="range" to="multi_zone_lidar_right/range" />
  </node>

</launch>