        <!-- realsense stereo camera-->
        <link name="realsense_link">
            <pose>0.096 0 -0.03 0 0 0</pose>
            <collision name="realsense_link_collision">
                <pose>0 0 0 -1.57 0 -1.57</pose>
                <geometry>
                    <box>
                        <size>0.02505 0.090 0.025</size>
                    </box>
                </geometry>
            </collision>
            <inertial>
                <pose>0 0 0 0 0 0</pose>
                <inertia>
                    <ixx>0.001</ixx>
                    <ixy>0.000</ixy>
                    <ixz>0.000</ixz>
                    <iyy>0.001</iyy>
                    <iyz>0.000</iyz>
                    <izz>0.001</izz>
                </inertia>
                <mass>0.001</mass>
            </inertial>

            <sensor name="cameradepth" type="depth">
                <camera name="camera">
                    <horizontal_fov>1.57</horizontal_fov>
                    <image>
                        <width>1280</width>
                        <height>720</height>
                    </image>
                    <clip>
                        <near>0.1</near>
                        <far>100</far>
